{
    "compilerOptions": {
      "jsx": "react",
      "target": "es6",
      "baseUrl": ".",
      "paths": {
        "*": [
          "resources/js/*"
        ],
        "components/*": [
          "resources/js/components/*"
        ],
        "helpers/*": [
          "resources/js/helpers/*"
        ],
        "redux/*": [
          "resources/js/redux/*"
        ]
      }
    },
    "exclude": ["node_modules/**"]
  }